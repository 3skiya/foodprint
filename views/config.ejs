    <% } %>
    <br>
      <table class="table table-hover table-sm">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Config ID</th>
            <th scope="col">Config Name</th>
            <th scope="col">Config Description</th>
            <th scope="col">Config Value</th>
            <th width="200px">Action</th>

          </tr>
        </thead>
        <tbody>
          <% if(data.length){

              for(var i = 0; i< data.length; i++) {%>
              <tr>
                <th scope="row"><%= (i+1) %></th>
                <td><%= data[i].configid%></td>
                <td><%= data[i].configname%></td>
                <td><%= data[i].configdescription%></td>
                <td><%= data[i].configvalue%></td>
                <td>
                <!--<a class="btn btn-success edit" href="../customers/edit/<%=data[i].id%>">Edit</a>-->
                <a class="btn btn-success edit btn-sm" href="#">Edit</a>
                <a class="btn btn-danger delete btn-sm" onclick="return alert('Are You sure?')" href="#">Delete</a>
                <!--<a class="btn btn-danger delete" onclick="return alert('Are You sure?')" href="../customers/delete/<%=data[i].id%>">Delete</a>-->
               </td>
              </tr>
              <% }

           }else{ %>
               <tr>
                  <td colspan="5">No Configuration</td>
               </tr>
            <% } %>

        </tbody>
    </table>
